<div class="row">
  <div class="col-12">
    <h4 style="color:rgb(9, 120, 231);padding-top: 3%; text-align:center;">ACTUALITZACIONS ONLINE DE LA COMUNITAT</h4>
  </div>
</div>
<div class="table-responsive">
  <table id="tableF" class="table table-bordered" style="font-size:medium;">
    <thead>
      <tr>
        <th scope="col">Data actualització</th>
        <th scope="col">Tipus</th>
        <th scope="col">Resultat</th>
      </tr>
    </thead>
    <tbody>
      {{#each rows}}
      <tr>
        <td>{{this.data}}</td>
        <td>{{this.tipus}}</td>
        <td>{{this.res}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>

